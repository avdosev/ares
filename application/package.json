{
  "name": "metida",
  "version": "0.2.0",
  "description": "Small blog-platform",
  "main": "src/app.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avdosev/metida"
  },
  "engines": {
    "node": "14.x"
  },
  "author": "K. company",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/avdosev/metida/issues"
  },
  "scripts": {
    "test": "npm run inspect:allConcurency",
    "start": "yarn run startjs",
    "start:prod": "yarn run serve",
    "serve": "ts-node src/app.ts",
    "startjs": "yarn run type && node dist/app.js",
    "type": "tsc",
    "lintFixWarnings": "eslint \"./**/*.js\" --fix",
    "inspect:lint": "eslint \"./**/*.js\" ",
    "inspect:mocha": "mocha ",
    "inspect:vulnerabilities": "npm audit",
    "inspect:all": " npm run inspect:vulnerabilities && npm run inspect:lint && npm run inspect:mocha ",
    "inspect:allConcurency": " concurrently \"npm run inspect:vulnerabilities\"  \"npm run inspect:lint\" \"npm run inspect:mocha\"  "
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.16.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^4.0.0",
    "express": "^4.14.0",
    "express-session": "^1.15.0",
    "express-validator": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "markdown-it": "^8.4.2",
    "mysql2": "^2.0.1",
    "morgan": "^1.10.0",
    "nodemailer": "^6.1.1",
    "sequelize": "^5.7.4",
    "ts-node": "^8.10.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.6",
    "@types/express-session": "^1.17.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/markdown-it": "^10.0.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "concurrently": "^4.1.1",
    "eslint": "^6.6.0",
    "lorem-ipsum": "^2.0.2",
    "mocha": "^6.1.4",
    "nodemon": "^2.0.4",
    "prettier": "1.17.0",
    "sequelize-cli": "^5.4.0",
    "typescript": "^3.9.5"
  }
}
